  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "lint": "eslint src --ext .ts,.tsx --max-warnings 0",
    "lint:ci": "eslint src --ext .ts,.tsx --max-warnings 100 --ignore-pattern '**/__tests__/**'",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "test": "cross-env CI=true react-scripts test --watchAll=false --passWithNoTests",
    "test:watch": "react-scripts test --watch",
    "eject": "react-scripts eject",
    "analyze": "npx source-map-explorer 'build/static/js/*.js'",
    "build:analyze": "CI=false npm run build && npm run analyze",
    "perf:check": "node scripts/performance-check.js --analyze-only",
    "perf:build": "node scripts/performance-check.js --build-and-analyze",
    "startup:check": "node scripts/startup-performance.js",
    "track:regression": "node scripts/track-regression.js",
    "track:stats": "node scripts/track-regression.js stats",
    "track:report": "node scripts/track-regression.js report"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
